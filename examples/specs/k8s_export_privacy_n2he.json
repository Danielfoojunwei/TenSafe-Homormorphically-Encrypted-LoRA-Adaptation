{
    "spec_version": "1.0",
    "run_name": "k8s-lora-n2he-full",
    "tenant_id": "enterprise-tenant",
    "execution_backend": "k8s_job_export",
    "training_method": "lora",
    "base_model_ref": "meta-llama/Llama-2-7b-hf",
    "dataset_source": {
        "source_type": "s3",
        "path": "s3://my-bucket/training-data/customer-support/",
        "split": "train",
        "text_column": "conversation"
    },
    "lora_config": {
        "r": 32,
        "lora_alpha": 64,
        "lora_dropout": 0.05,
        "target_modules": [
            "q_proj",
            "k_proj",
            "v_proj",
            "o_proj"
        ],
        "bias": "none"
    },
    "training_params": {
        "epochs": 5,
        "batch_size": 8,
        "learning_rate": 0.0001,
        "warmup_steps": 200,
        "max_seq_length": 2048,
        "gradient_accumulation_steps": 8,
        "seed": 42,
        "fp16": true
    },
    "promotion_policy": {
        "threshold": 0.92,
        "forgetting_budget": 0.08,
        "regression_budget": 0.03
    },
    "release_policy": {
        "route_key": "enterprise-customer-support",
        "channel": "staging",
        "rollback_enabled": true
    },
    "trust": {
        "trust_core": "nitro_enclave",
        "attestation_required": true
    },
    "privacy": {
        "mode": "n2he",
        "n2he_profile": "router_plus_eval",
        "n2he_sidecar": "enabled",
        "n2he_encrypted_logs": true,
        "n2he_service_endpoint": null
    },
    "outputs": {
        "tgsp_required": true,
        "evidence_required": true,
        "registry_required": true,
        "merged_export": false
    },
    "labels": {
        "environment": "staging",
        "team": "ml-platform",
        "privacy": "n2he-full",
        "trust": "nitro-attested"
    },
    "annotations": {
        "description": "K8s export with full N2HE privacy (router + eval). Uses Nitro Enclave for attestation and N2HE sidecar for encrypted operations.",
        "k8s_namespace": "tensorguardflow",
        "k8s_service_account": "tgflow-runner",
        "n2he_note": "Sidecar container co-scheduled for HE operations. All routing and evaluation done on encrypted data."
    }
}